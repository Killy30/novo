<head>
    <link rel="stylesheet" href="../../../css/estudiante/list_groupe.css">
</head>

<%groupe_.forEach(groupe =>{%>
    <a class="a_groupe" href="/groupe-student/<%=groupe.id%>">
        <div class="groupe" >
            <div class="box_element">
                <div class="name_groupe">
                    <p><%=groupe.name%> <%=groupe.nivel%></p>
                </div>
                <div class="description">
                    <p><%=groupe.description%></p>
                </div>
                <div class="fooder_groupe">
                    <p><%=groupe.teacher.name%> <%=groupe.teacher.lastName%></p>
                </div>
            </div>
        </div>
    </a>
<%})%>